    rdfs:domain      xtime: ;
    rdfs:range       xsd:nonNegativeInteger ;
    rdfs:isDefinedBy xtime: .

xtime:
    xtime:weekInHours "168"^^xsd:nonNegativeInteger ; .

xtime:weekInMinutes
    a                owl:DatatypeProperty ;
    rdfs:domain      xtime: ;
    rdfs:range       xsd:nonNegativeInteger ;
    rdfs:isDefinedBy xtime: .

xtime:
    xtime:weekInMinutes "10080"^^xsd:nonNegativeInteger ; .

xtime:weekInSeconds
    a                owl:DatatypeProperty ;
    rdfs:domain      xtime: ;
    rdfs:range       xsd:nonNegativeInteger ;
    rdfs:isDefinedBy xtime: .

xtime:
